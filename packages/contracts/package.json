{
  "name": "@zerodao/contracts",
  "version": "0.1.0",
  "description": "zeroDAO Protocol Contracts",
  "repository": "https://github.com/zerodao-finance/zero-protocol",
  "scripts": {
    "prettier:sol": "prettier --write ./**/*.sol",
    "verify": "hardhat etherscan-verify --license MIT",
    "verify:implementation": "bash -c 'hardhat verify `node scripts/get-implementation` --libraries scripts/libraries.js'",
    "test": "hardhat test",
    "test:badger": "yarn hardhat test test/test-badger.ts --network localhost",
    "test:badger:ethereum": "CHAIN=ETHEREUM yarn test:badger",
    "test:badger:arbitrum": "BADGER=true CHAIN=ARBITRUM yarn test:badger",
    "test:ren": "CHAIN=ETHEREUM yarn hardhat test test/test-renv3.ts --network localhost",
    "script:keeper": "hardhat run scripts/keeper.js",
    "start": "hardhat node",
    "lint:check": "prettier --check 'contracts/**/*.sol' && hardhat compile --config ./hardhat.config.ts && npx solhint --config ./config/.solhint.json --ignore-path ./config/.solhintignore 'contracts/**/*.sol'",
    "lint:fix": "prettier --write 'contracts/**/*.sol'",
    "build:sol": "hardhat compile",
    "build": "tsc -p ./tsconfig.json",
    "node:arbitrum": "CHAIN=ARBITRUM FORKING=true hardhat node",
    "node:ethereum": "CHAIN=ETHEREUM FORKING=true hardhat node --export-all  deployments/deployments.json",
    "node:badger:ethereum": "CHAIN=ETHEREUM FORKING=true hardhat node",
    "node:badger:arbitrum": "CHAIN=ARBITRUM FORKING=true hardhat node",
    "script:harvester": "hardhat run scripts/harvester.js",
    "gen:coders": "stack-packer ./struct-defs ./contracts/erc4626/utils ./contracts/test/ ./test/types/"
  },
  "author": "zeroDAO",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@boringcrypto/boring-solidity": "https://github.com/boringcrypto/BoringSolidity",
    "@ethersproject/cli": "^5.5.0",
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@gnosis.pm/safe-core-sdk": "^2.0.0",
    "@gnosis.pm/safe-ethers-lib": "^1.0.0",
    "@gnosis.pm/safe-service-client": "^1.1.1",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "3.4.0",
    "@openzeppelin/contracts-new": "npm:@openzeppelin/contracts",
    "@openzeppelin/contracts-upgradeable": "3.4.1-solc-0.7-2",
    "@renproject/ren": "^2.5.13",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/bignumber.js": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@types/object-hash": "^2.1.1",
    "@types/sinon": "^10.0.2",
    "@uniswap/merkle-distributor": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.1",
    "aegir": "^36.1.3",
    "chai": "^4.3.4",
    "core-js-pure": "^3.15.1",
    "ethereum-waffle": "^3.4.4",
    "ethers-polygongastracker": "https://github.com/freelogstudio/ethers-polygongastracker",
    "fetch-mock": "^9.11.0",
    "ganache-cli": "^6.12.2",
    "hardhat": "^2.8.4",
    "hardhat-deploy": "^0.9.3",
    "hardhat-gas-reporter": "^1.0.4",
    "nice-repl": "https://github.com/raymondpulver/nice-repl",
    "oz410": "npm:@openzeppelin/contracts@3.4.1-solc-0.7-2",
    "oz460": "npm:@openzeppelin/contracts@4.6.0",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "request-debug": "^0.2.0",
    "sinon": "^11.1.1",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "stack-packer": "^1.1.6",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "4.4.4",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "@0x/utils": "^6.4.3",
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/hash": "^5.4.0",
    "@ethersproject/random": "^5.4.0",
    "@ethersproject/strings": "^5.4.0",
    "@ethersproject/transactions": "^5.4.0",
    "@nomiclabs/buidler": "^1.4.8",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@rari-capital/solmate": "^6.4.0",
    "@renproject/chains": "^2.5.13",
    "@renproject/chains-ethereum": "^2.5.13",
    "@types/node": "^16.4.1",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "eip712-helpers": "^1.0.2",
    "ethers": "^5.6.1",
    "ethers-gasnow": "https://github.com/kungfuflex/ethers-gasnow",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-preprocessor": "^0.1.4",
    "keccak256": "^1.0.6",
    "peer-info": "^0.17.5",
    "pull-stream": "^3.6.14",
    "tslint-config-prettier": "^1.18.0",
    "winston": "^3.3.3"
  }
}
